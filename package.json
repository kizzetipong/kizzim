{
  "name": "kizzim",
  "version": "0.0.1",
  "description": "Backtest Simulator used by TR infra internally",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && babel-node src/server.js",
    "start:dev": "set NODE_ENV=development && npm run build:dev && nodemon --exec babel-node -- src/server.js",
    "start:universal": "set UNIVERSAL=true && npm run start",
    "start:dev:universal": "set NODE_ENV=development && set UNIVERSAL=true && npm run start:dev",
    "build": "NODE_ENV=production webpack -p",
    "build:dev": "webpack -d",
    "build:dev:watch": "webpack -d --watch"
  },
  "author": "Kittipong Boonyapisomparn",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "ejs": "^2.5.2",
    "express": "5.0.0-alpha.5",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-router-dom": "^4.0.0"
  },
  "devDependencies": {
    "babel-loader": "^6.4.1",
    "babel-register": "^6.18.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "gulp-sass": "^3.1.0",
    "nodemon": "^1.11.0",
    "webpack": "2.2.1"
  }
}
